<!DOCTYPE html>
<html>
    <head>
        <title>Larger Number</title>
    </head>
    <body>
        <script>
            let a=parseInt(prompt("Enter first number:"));
            let b=parseInt(prompt("Enter second number:"));
            let c=parseInt(prompt("Enter third number:"));
            if (a>b && a>c) {
                document.write(a + "LARGER NUMBER");
                alert(a + "LARGER NUMBER");
            }
            else if(b>a && b>c) {
                document.write(b + "LARGER NUMBER");
                alert(b + "LARGER NUMBER");
            }
            else if(c>a && c>b) {
                document.write(c + "LARGER NUMBER");
                alert(c + "LARGER NUMBER");
            }
            else {
                document.write("EQUAL NUMBER");
                alert("EQUAL NUMBER");
            }
        </script>
    </body>
</html>
